{
BitmapFont: {
	cyrFont: {
		file: cyrFont.fnt
		scaledSize: -1
		markupEnabled: false
		flip: false
	}
	font: {
		file: font-export.fnt
		scaledSize: -1
		markupEnabled: false
		flip: false
	}
	title: {
		file: font-title-export.fnt
		scaledSize: -1
		markupEnabled: false
		flip: false
	}
}
Color: {
	black: {
		r: 0
		g: 0
		b: 0
		a: 1
	}
	orange: {
		r: 0.91
		g: 0.7078283
		b: 0
		a: 1
	}
	white: {
		r: 1.0
		g: 1.0
		b: 1.0
		a: 1.0
	}
	yellow: {
		r: 1
		g: 0.8980392
		b: 0.4
		a: 1
	}
}
TintedDrawable: {
	red: {
		name: white
		color: {
			r: 0.66666675
			g: 0.07555556
			b: 0.07555556
			a: 0.0
		}
	}
}
ButtonStyle: {
	default: {
		up: button
		down: button-pressed
		over: button-over
	}
}
CheckBoxStyle: {
	default: {
		checkboxOn: checkbox-on
		checkboxOff: checkbox-off
		checkboxOver: checkbox-off-over
		font: cyrFont
		fontColor: yellow
		overFontColor: orange
	}
	radio: {
		checkboxOn: radio-on
		checkboxOff: radio-off
		checkboxOver: radio-off-over
		font: cyrFont
		fontColor: yellow
		overFontColor: orange
	}
}
ImageButtonStyle: {
	default: {
		imageUp: button
		imageDown: button-pressed
		imageOver: button-over
	}
}
ImageTextButtonStyle: {
	default: {
		font: font
		fontColor: yellow
		overFontColor: orange
		up: button
		down: button-pressed
		over: button-over
	}
	checkbox: {
		imageUp: checkbox-off
		imageDown: checkbox-off-pressed
		imageOver: checkbox-off-over
		imageChecked: checkbox-on
		imageCheckedOver: checkbox-on-over
		font: cyrFont
		fontColor: yellow
		overFontColor: orange
	}
	radio: {
		imageUp: radio-off
		imageDown: radio-off-pressed
		imageOver: radio-off-over
		imageChecked: radio-on
		imageCheckedOver: radio-on-over
		font: cyrFont
		fontColor: yellow
		overFontColor: orange
	}
}
LabelStyle: {
	default: {
		font: cyrFont
		fontColor: yellow
	}
	title: {
		font: title
	}
}
ListStyle: {
	default: {
		font: cyrFont
		fontColorSelected: white
		fontColorUnselected: yellow
		selection: red
		background: list
	}
}
ProgressBarStyle: {
	default-horizontal: {
		background: progress-bar
		knobBefore: progress-bar-life
	}
	default-vertical: {
		background: progress-bar-v
		knobBefore: progress-bar-lfe-v
	}
	mana: {
		background: progress-bar
		knobBefore: progress-bar-mana
	}
	mana-vertical: {
		background: progress-bar-v
		knobBefore: progress-bar-mana-v
	}
	health-orb: {
		background: health-orb
		knobBefore: health-orb-fill
	}
	mana-orb: {
		background: mana-orb
		knobBefore: mana-orb-fill
	}
}
ScrollPaneStyle: {
	default: {
		hScrollKnob: scrollbar-horizontal
		vScrollKnob: scrollbar-vertical
	}
}
SelectBoxStyle: {
	default: {
		font: cyrFont
		fontColor: yellow
		background: select-box
		scrollStyle: default
		listStyle: default
		backgroundOver: select-box-highlighted
		backgroundOpen: select-box-pressed
	}
}
SliderStyle: {
	default-horizontal: {
		knobOver: slider-knob-over
		knobDown: slider-knob-over
		background: slider
		knob: slider-knob
	}
	default-vertical: {
		knobOver: slider-knob-over
		knobDown: slider-knob-over
		background: slider
		knob: slider-knob
	}
}
SplitPaneStyle: {
	default-horizontal: {
		handle: split-pane-horizontal
	}
	default-vertical: {
		handle: split-pane-vertical
	}
}
TextButtonStyle: {
	default: {
		font: cyrFont
		fontColor: yellow
		overFontColor: orange
		up: button
		down: button-pressed
		over: button-over
	}
}
TextFieldStyle: {
	default: {
		font: cyrFont
		fontColor: yellow
		focusedFontColor: orange
		background: textfield
		cursor: cursor-primed
		selection: red
	}
}
TextTooltipStyle: {
	default: {
		label: default
		background: textfield
	}
}
TouchpadStyle: {
	default: {
		background: touchpad
		knob: slider-knob
	}
}
TreeStyle: {
	default: {
		plus: plus
		minus: minus
		selection: red
	}
}
WindowStyle: {
	default: {
		background: window
		titleFont: cyrFont
		stageBackground: white
	}
	small: {
		background: textfield
		titleFont: cyrFont
	}
}
}